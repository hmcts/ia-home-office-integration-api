{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ccdCaseId": {
      "type": "number",
      "pattern": "^[0-9]{16}$"
    },
    "uan": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}$"
    },
    "familyName": {
    "type": "string"
    },
    "givenNames": {
    "type": "string"
    },
    "dateOfBirth": {
    "type": "string",
    "pattern": "^(19[0-9]{2}|[2-9][0-9]{3})-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])$"
    },
    "stf24weeks": {
      "type": "boolean"
    },
    "timeStamp": {
      "type": "string",
      "pattern": "^(19[0-9]{2}|[2-9][0-9]{3})-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])$"
    }
  },
  "required": [
    "ccdCaseId",
    "uan",
    "familyName",
    "givenNames",
    "dateOfBirth",
    "stf24weeks",
    "timeStamp"
  ]
}