
# Add @Generated annotation to all Lombok-generated code
# This tells SonarQube and other tools to skip generated methods
lombok.addLombokGeneratedAnnotation = true

# Optionally add @SuppressWarnings to generated code
# lombok.addSuppressWarnings = true

# Make equals and hashCode call super by default (optional)
# lombok.equalsAndHashCode.callSuper = call

# Configure other Lombok defaults as needed
config.stopBubbling = true
